<h2>Welcome, <%= @user.username %></h2>

<h3>All Tweets</h3>
<ul>
  <% Tweet.all.each do |tweet| %>
    <li>
      <%= tweet.content %>
      <a href="/tweets/<%=tweet.id%>">show</a>
    </li>

    <form action="/tweets/<%=tweet.id%>/delete" method="post"> <br>
     <input type="hidden" name="_method" value="delete" id='hidden'>
     <input type="submit" value="Delete tweets">
    </form>

  <% end %>
</ul>
<a href="/logout">Log out</a>
